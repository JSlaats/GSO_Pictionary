<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="mainPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientGUI.LoginScreen">
   <children>
      <Button fx:id="loginBtn" layoutX="190.0" layoutY="246.0" mnemonicParsing="false" onAction="#Login" prefHeight="65.0" prefWidth="220.0" text="Log in!" />
      <Button fx:id="registerBtn" layoutX="190.0" layoutY="323.0" mnemonicParsing="false" onAction="#Register" prefHeight="25.0" prefWidth="220.0" text="Register" />
      <Label layoutX="130.0" layoutY="40.0" text="Pictionary">
         <font>
            <Font name="Comic Sans MS Bold" size="72.0" />
         </font>
      </Label>
      <TextField fx:id="inputUsername" layoutX="261.0" layoutY="161.0" />
      <TextField fx:id="inputPassword" accessibleRole="PASSWORD_FIELD" layoutX="261.0" layoutY="200.0" />
      <Label layoutX="190.0" layoutY="165.0" text="Username:" />
      <Label layoutX="190.0" layoutY="204.0" text="Password:" />
   </children>
</AnchorPane>
